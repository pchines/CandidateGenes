%p#notice= notice

%p
  %b Symbol:
  = @gene.symbol
%p
  %b Aliases:
  = @gene.gene_aliases
  %b
    = link_to 'Add', new_gene_alias_path(@gene)
%p
  %b Long name:
  = @gene.long_name
%p
  %b Summary:
  = @gene.summary
%p
  %b Assigned to:
  = @gene.assigned_to
%p
  %b Overall score:
  = @gene.score

= render 'feature_table'

%h2 Add a Feature

= render 'features/new'

= link_to 'Edit', edit_gene_path(@gene)
\|
= link_to 'Back', genes_path
